{
  "name": "fpnews",
  "version": "0.0.1",
  "private": true,
  "repository": "https://github.com/gbenga2540/lendsqr-fp-news.git",
  "author": "gbenga2540",
  "license": "MIT",
  "scripts": {
    "kill:node": "killall node",
    "kill:java": "killall -9 java",
    "kill:all": "yarn kill:java && yarn kill:node",
    "start": "react-native start",
    "start:reset": "yarn start --reset-cache",
    "android": "react-native run-android",
    "android:apk": "cd android && ./gradlew app:assembleDebug",
    "android:apk:prod": "cd android && ./gradlew app:assembleRelease",
    "android:apk:open": "open android/app/build/outputs/apk/release",
    "android:apk:prod:full": "yarn clean:android && yarn android:apk:prod && yarn:android:apk:open",
    "android:bundle": "cd android && ./gradlew app:bundleDebug",
    "android:bundle:prod": "cd android && ./gradlew app:bundleRelease",
    "gen-asr": "cd android && ./gradlew signingReport",
    "stop:gradle": "cd android && ./gradlew --stop",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "ios": "react-native run-ios",
    "ios:bundle:assets": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:build": "yarn ios:bundle:assets && cd ios && xcodebuild -workspace fpnews.xcworkspace -scheme fpnews -configuration Release -derivedDataPath build",
    "pod:install": "cd ios && pod install",
    "pod:update": "cd ios && pod update",
    "pod:reset": "cd ios && pod deintegrate && pod setup && pod install",
    "xcode": "cd ios && xed .",
    "clean:node": "rm -rf node_modules && yarn cache clean && yarn install && echo 'Clean:Node Completed'",
    "clean:ios": "rm -rf ios/build && cd ios && xcodebuild clean && cd .. && rm -rf ~/Library/Developer/Xcode/DerivedData && watchman watch-del-all && yarn cache clean && echo 'Clean:iOS Completed'",
    "clean:android": "cd android && ./gradlew clean && echo 'Clean:Android Completed'",
    "clean": "yarn clean:node && yarn clean:ios && yarn clean:android",
    "lint": "eslint .",
    "lint-src": "eslint src",
    "test": "jest",
    "svg": "node ./src/codegen/svg-gen.js"
  },
  "dependencies": {
    "@react-native-firebase/analytics": "^20.1.0",
    "@react-native-firebase/app": "^20.1.0",
    "@react-native-firebase/auth": "^20.1.0",
    "@react-native-firebase/crashlytics": "^20.1.0",
    "@react-native-firebase/messaging": "^20.1.0",
    "@react-native-firebase/perf": "^20.1.0",
    "@react-native-firebase/remote-config": "^20.1.0",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.5",
    "appcenter": "^5.0.1",
    "appcenter-analytics": "^5.0.1",
    "appcenter-crashes": "^5.0.1",
    "axios": "^1.7.2",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-code-push": "^8.2.2",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-permissions": "^4.1.5",
    "react-native-render-html": "^6.3.4",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.10.4",
    "react-native-screens": "^3.31.1",
    "react-native-share": "^10.2.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.3.0",
    "react-native-toast-message": "^2.2.0",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.84",
    "@react-native/eslint-config": "0.74.84",
    "@react-native/metro-config": "0.74.84",
    "@react-native/typescript-config": "0.74.84",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@tsconfig/react-native": "^3.0.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/validator": "^13.12.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "handlebars": "^4.7.8",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.4.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.5",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
